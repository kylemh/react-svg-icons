{
  "name": "svg-icons",
  "version": "1.0.0",
  "description": "Componetized SVG Icons for React",
  "main": "dist/index.production.cjs.js",
  "module": "dist/index.production.esm.js",
  "author": "Kyle Holmberg",
  "license": "MIT",
  "keywords": [
    "react",
    "svg",
    "icons",
    "svg"
  ],
  "scripts": {
    "build": "yarn build:dev && yarn build:prod",
    "build:dev": "yarn rollup -c --environment BUILD:development",
    "build:prod": "yarn rollup -c --environment BUILD:production",
    "format": "yarn prettier --write \"**/*.{js,json,md,html,css}\"",
    "prepublishOnly": "yarn build:dev && yarn test",
    "storybook": "yarn build:prod && start-storybook",
    "test": "yarn build:prod && yarn jest"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@innocuous/functions": "^1.3.0",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-storyshots": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@testing-library/react": "^8.0.8",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^9.2.1",
    "path": "^0.12.7",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-svg-loader": "2.1.0",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.19.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-react-svg": "2.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{js,json,md,html,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
